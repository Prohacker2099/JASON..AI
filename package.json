    "@types/serialport": "^8.x",
    "@types/ws": "^8.18.1",
    "concurrently": "^8.x",
    "cross-env": "^10.0.0",
    "puppeteer": "^21.x",
    "react-scripts": "^5.0.1",
    "sass": "^1.90.0",
    "tsx": "^4.x",
    "typescript": "^5.x"
