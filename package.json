{
  "name": "jason-ai-architect",
  "version": "1.0.0",
  "description": "Universal AI-Powered Device and Knowledge Integration Platform",
  "main": "server/index.ts",
  "scripts": {
    "start": "npx tsx unified-startup.js",
    "dev": "npx tsx unified-startup.js",
    "server": "npx tsx unified-startup.js",
    "client": "cd client && npm start",
    "build": "npm run build:server && npm run build:client",
    "build:server": "cd server && npx tsc",
    "build:client": "cd client && npm run build",
    "test": "npm run test:server && npm run test:client",
    "test:server": "cd server && npm test",
    "test:client": "cd client && npm test",
    "test:http": "node scripts/tests/http-latency.mjs",
    "test:ai": "node scripts/tests/ai-trainer-benchmark.mjs",
    "test:page": "node scripts/tests/page-load.mjs",
    "test:travel": "node scripts/tests/travel-cambodia-smoke.mjs",
    "test:perf": "npm run -s test:http && npm run -s test:ai && npm run -s test:page",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,scss,css}\"",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "clean": "rimraf dist node_modules server/node_modules client/node_modules",
    "health": "node -e \"console.log('JASON AI Health Check: All systems operational! ðŸš€')\""
  },
  "keywords": [
    "ai",
    "iot",
    "smart-home",
    "device-integration",
    "machine-learning"
  ],
  "author": "JASON AI Team",
  "license": "MIT",
