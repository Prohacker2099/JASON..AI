    "@prisma/client": "^6.19.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.20.0",
    "axios": "^1.x",
    "dotenv": "^16.x",
    "express": "^4.x",
    "fast-csv": "^5.x",
    "mailparser": "^3.9.0",
    "moment": "^2.x",
    "mqtt": "^5.x",
    "multer": "^1.x",
    "node-fetch": "^3.x",
    "node-ical": "^0.17.x",
    "nodemailer": "^7.0.11",
    "prisma": "^6.13.0",
    "puppeteer": "^21.x",
    "react": "^18.x",
    "react-dom": "^18.x",
    "react-router-dom": "^6.x",
    "rimraf": "^5.x",
    "serialport": "^12.x",
    "sqlite": "^5.x",
    "sqlite3": "^5.x",
    "tailwindcss-animate": "^1.0.7",
    "winston": "^3.13.0",
    "ws": "^8.x"
  },
